{
  "name": "kapokyue/laravel-excel",
  "description": "Supercharged Excel exports in Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "phpexcel",
    "excel",
    "csv",
    "export",
    "import",
    "batch"
  ],
  "authors": [
    {
      "name": "Maatwebsite.nl",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "kapokyue/phpexcel": "dev-feature/php74",
    "illuminate/cache": "^7.8.1",
    "illuminate/config": "^7.8.1",
    "illuminate/filesystem": "^7.8.1",
    "illuminate/support": "^7.8.1",
    "jeremeamia/superclosure": "^2.3",
    "nesbot/carbon": "~2.32.2",
    "tijsverkoyen/css-to-inline-styles": "~2.0",
    "laravel/helpers": "^1.2"
  },
  "require-dev": {
    "phpseclib/phpseclib": "~1.0",
    "phpunit/phpunit": "^8.5",
    "mockery/mockery": "^1.3.1",
    "orchestra/testbench": "^5.1.0"
  },
  "suggest": {
    "illuminate/http": "^7.8.1",
    "illuminate/routing": "^7.8.1",
    "illuminate/view": "^7.8.1",
    "illuminate/queue": "^7.8.1"
  },
  "autoload": {
    "classmap": [
      "src/Maatwebsite/Excel"
    ],
    "psr-0": {
      "Maatwebsite\\Excel\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Maatwebsite\\Excel\\ExcelServiceProvider"
      ],
      "aliases": {
        "Excel": "Maatwebsite\\Excel\\Facades\\Excel"
      }
    }
  }
}
